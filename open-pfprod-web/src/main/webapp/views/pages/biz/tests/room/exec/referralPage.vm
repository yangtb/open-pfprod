    <style>
        #none {
            width: 140px;
            margin: 122px auto 0;
            height: 500px;
            display: block;
        }

        #none img {
            margin: 0 6px;
        }

        #none p:nth-child(2)  {
            width:108px;
            color:rgba(51,51,51,1);
            margin: 20px auto 28px;
            height:19px;
            font-size:18px;
            font-family:MicrosoftYaHei;
            font-weight:400;
        }

        #none p:last-child {
            color:rgba(153,153,153,1);
            width:140px;
            height:14px;
            font-size:14px;
            font-family:MicrosoftYaHei;
            font-weight:400;
        }

        .fix {
            position: relative;
            margin: 10px 0 16px 10px;
        }

        .add-but:hover {
            cursor: pointer;
        }

        .add-but {
            width:90px;
            border: none;
            height:32px;
            background:rgba(55,141,126,1);
            border-radius:4px;
        }

        .add-but span {
            width: 28px;
            height: 14px;
            display: inline-block;
            font-size: 14px;
            font-family: MicrosoftYaHei;
            font-weight: 400;
            margin-left: 45px;
            margin-right: 17px;
            color: rgba(255, 255, 255, 1);
        }

        .vertical {
            top: 9px;
            left: 25px;
            width: 3px;
            height: 15px;
            position: absolute;
            background-color: #ffffff;
        }

        .horizontal {
            top: 15px;
            left: 19px;
            width: 15px;
            height: 3px;
            position: absolute;
            background-color: #ffffff;
        }

        .main {
            height: 628px;
            overflow-y: auto;
            margin-left: 10px;
            border-radius: 4px;
        }

        .disc {
            width:8px;
            height:8px;
            display: inline-block;
            background:rgba(55,141,126,1);
            margin-right: 3px;
            border-radius:50%;
        }

        .diagnose-title {
            color:rgba(51,51,51,1);
            width:77px;
            height:14px;
            font-size:14px;
            font-family:MicrosoftYaHei;
            font-weight:400;
        }


        .referral {
            height:290px;
            border:1px solid rgba(239,241,244,1);
            background:rgba(255,255,255,1);
            border-radius:4px;
            margin-bottom: 20px;
        }

        .titile {
            margin: 15px 0 0 16px;
        }

        .layui-card-body {
            padding: 0;
            margin: 0 15px;
        }

        .layui-table-view {
            margin: 0;
        }

        .button-right {
            float: right;
            margin-top: 10px;
        }

    </style>

</head>

<body class="wrapper-content">

<div style="overflow:hidden">

    <div class="fix">
        <div class="vertical"></div>
        <div class="horizontal"></div>
        <button id="searchAnswer" type="button" class="add-but">
            <span>新增</span>
        </button>
        <input id="idDie" name="idDie" hidden>
    </div>




    <div class="main" id="zdField">
        <div id="none">
            <img src="${contextPath}/public/layui/build/images/none.png"/>
            <p>暂无拟诊内容</p>
            <p>请点击新增添加拟诊项</p>
        </div>
    </div>

</div>


<script src="$!{contextPath}/public/layui/plugins/layui/layui.js"></script>
<script src="${contextPath}/public/common/js/jquery.min.js"></script>
<script src="$!{contextPath}/public/biz/js/biz/tests/room/exec/execReferralController.js"></script>


<script type="text/html" id="summaryBar">
    <button type="button" class="layui-btn layui-btn-xs layui-btn-danger" lay-event="del" title="删除">
