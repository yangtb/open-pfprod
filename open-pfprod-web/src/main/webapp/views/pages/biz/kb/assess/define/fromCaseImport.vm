<!DOCTYPE html>
<html>

<head>
    <meta charset="utf-8">
    <title>从病例引入</title>
    <meta name="renderer" content="webkit">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
    <link rel="stylesheet" href="${contextPath}/public/layui/plugins/layui/css/layui.css" media="all">
    <link rel="stylesheet" href="${contextPath}/public/ztree/zTreeStyle/zTreeStyle.css" type="text/css">
    <link rel="stylesheet" href="${contextPath}/public/biz/css/common.css" type="text/css">
    <link rel="stylesheet" href="${contextPath}/public/biz/css/mouseRight.css" type="text/css">
    <link rel="stylesheet" type="text/css" href="${contextPath}/public/biz/iconfont/iconfont.css">
</head>

<script>
    var basePath = '$!{basePath}';
    var contextPath = '$!{contextPath}';
    var caseTag = '$!{caseTag}';
    var cdEvaAsse = '$!{cdEvaAsse}';
    var idEvaCase = '$!{idEvaCase}';
    var tagFlag='$!{tagFlag}';
    var idMedicalrec = '$!{idMedicalrec}';
    var idTag ='$!{idTag}';
    var caseName ='$!{caseName}';
    var module ='$!{module}';
</script>

<body class="body-my">

<div class="wrapper-content">
    <div class="layui-tab" lay-filter="tagTabFilter">
        <ul class="layui-tab-title">
            <li class="layui-this">问诊</li>
            <li>体格检查</li>
            <li>辅助检查</li>
        </ul>
        <div class="layui-tab-content">
            <div class="layui-tab-item layui-show">
                <form class="layui-form">
                    <div class="layui-inline">
                        <div class="layui-input-inline">
                            <input id="idInquesCaSearch" name="idInquesCa" type="text"
                                   class="layui-input" autocomplete="off" readonly/>
                        </div>
                    </div>
                    <div class="layui-inline">
                        <div class="layui-input-inline">
                            <input type="text" name="consKeyword" id="consKeyword" autocomplete="off" class="layui-input"
                                   placeholder="请输入问题">
                        </div>
                    </div>
                    <div class="layui-inline">
                        <div class="layui-input-inline">
                            <button type="button" class="layui-btn" lay-submit lay-filter="consQueryFilter">
                                <i class="iconfont icon-query"></i>查询
                            </button>
                            <button id="consImport" type="button" class="layui-btn layui-btn-normal">
                                引入
                            </button>
                        </div>
                    </div>
                </form>
                <table id="partConsTable" lay-filter="partConsTableFilter">
                </table>
            </div>

            <div class="layui-tab-item">
                <form class="layui-form">
                    <div class="layui-inline">
                        <div class="layui-input-inline">
                            <input id="idBodyCaSearch" name="idBodyCa" type="text"
                                   class="layui-input" autocomplete="off" readonly/>
                        </div>
                    </div>
                    <div class="layui-inline">
                        <div class="layui-input-inline">
                            <input type="text" name="checkKeyword" id="checkKeyword" autocomplete="off" class="layui-input"
                                   placeholder="部位描述">
                        </div>
                    </div>
                    <div class="layui-inline">
                        <div class="layui-input-inline">
                            <button type="button" class="layui-btn" lay-submit lay-filter="checkQueryFilter">
                                <i class="iconfont icon-query"></i>查询
                            </button>
                            <button id="checkImport" type="button" class="layui-btn layui-btn-normal">
                                引入
                            </button>
                        </div>
                    </div>
                </form>
                <table id="partCheckTable" lay-filter="partCheckTableFilter">
                </table>
            </div>
            <div class="layui-tab-item">
                <form class="layui-form">
                    <div class="layui-inline">
                        <div class="layui-input-inline">
                            <input id="idInspectSearch" name="idInspect" type="text"
                                   class="layui-input" autocomplete="off" readonly/>
                        </div>
                    </div>
                    <div class="layui-inline">
                        <div class="layui-input-inline">
                            <input type="text" name="examKeyword" id="examKeyword" autocomplete="off" class="layui-input"
                                   placeholder="请输入项目">
                        </div>
                    </div>
                    <div class="layui-inline">
                        <div class="layui-input-inline">
                            <button type="button" class="layui-btn" lay-submit lay-filter="examQueryFilter" >
                                <i class="iconfont icon-query"></i>查询
                            </button>
                            <button id="examImport" type="button" class="layui-btn layui-btn-normal">
                                引入
                            </button>
                        </div>
                    </div>
                </form>
                <table id="partExamTable" lay-filter="partExamTableFilter">
                </table>
            </div>
        </div>
    </div>

</div>

<script src="${contextPath}/public/layui/plugins/layui/layui.js"></script>
<script src="${contextPath}/public/biz/js/biz/kb/assess/fromCaseImportController.js"></script>

<script type="text/html" id="inquisitionQuestion">
    <a class="layui-btn layui-btn-normal layui-btn-xs" lay-event="edit"><i class="iconfont icon-edit"></i> 编辑</a>
    <a class="layui-btn layui-btn-normal layui-btn-xs" lay-event="editAnswer"><i class="layui-icon layui-icon-edit"></i>编辑答案</a>
</script>

</body>
</html>